<Page
    x:Class="Playground.Views.ParamsTest.ShowParamsTestPage"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:local="using:Playground.Views.ParamsTest"
    Background="{ThemeResource ApplicationPageBackgroundThemeBrush}">

  <Grid>
    <StackPanel Spacing="10">
      <TextBlock>
        Route Integer Value: <Run Text="{x:Bind Model.IntValue}"/>
      </TextBlock>

      <TextBlock>
        Route String Value: <Run Text="{x:Bind Model.StringValue}"/>
      </TextBlock>

      <Button Content="Go Back" Command="{x:Bind Model.GoBackCommand}"/>
    </StackPanel>
  </Grid>
</Page>
